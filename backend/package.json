{
  "name": "Comapas_recovery",
  "version": "0.3.0",
  "description": "Comapas_recovery project on ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "start": "node dist",
    "prestart": "npm run build",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "dev": "ENV=dev nodemon --max_old_space_size=8192 -w src --exec \"babel-node src --presets es2015,stage-0\""
  },
  "repository": "developit/express-es6-rest-api",
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "babel": "^6.23.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-validator": "^5.3.0",
    "glob": "^7.1.3",
    "http": "0.0.0",
    "jsonwebtoken": "^8.3.0",
    "md5": "^2.2.1",
    "mongoose": "^5.3.12",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "nodemon": "^1.14.12"
  }
}
